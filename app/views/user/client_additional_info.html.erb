<%= form_with url: client_patch_additional_info_path do |f| %>
    <div class="field">
        <%= f.label :mobile_number %><br />
        <%= f.text_field :mobile_number%>
    </div>

    <div class="field">
        <%= f.label :longitude %><br/>
        <%= f.number_field :longitude %>
    </div>

    <div class="field">
        <%= f.label :latitude %><br/>
        <%= f.number_field :latitude %>
    </div>
 
    <div class="actions">
        <%= f.submit 'Update' %>
    </div>

 <% end %>